{
  "expo": {
    "name": "Health Connect",
    "slug": "project-health-connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/connectlogo.png",
    "scheme": "projecthealthconnect",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDB4Yr4oq_ePtBKd8_HZSEd0_xi-UId6Fg"
        }
      },
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/connectlogo.png",
        "backgroundImage": "./assets/images/connectlogo.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "net.kopanovertex.healthConnect",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "./plugins/withNetworkSecurityConfig",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/connectlogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to connect you with nearby healthcare providers."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/connectlogo.png",
          "color": "#10B981",
          "sounds": [],
          "mode": "production"
        }
      ],
      "expo-build-properties",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "37466beb-b202-424c-a1ab-fa3b1a504a68"
      }
    }
  }
}
